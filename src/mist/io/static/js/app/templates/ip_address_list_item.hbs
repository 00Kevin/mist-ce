<td>{{ipaddress}}</td>
<td>
    <div class="reserved">
        <span class="{{if isTogglingReserve 'ui-state-disabled'}}">
            {{#if isTogglingReserve}}
                <span class="ajax-loader"></span>
            {{/if}}
            <select name="reserved-slider" data-role="slider" data-mini="true"
                {{action "reservedToggled" target=view on="change"}}>
                <option value="off"></option>
                <option value="on" selected={{reserved}}>Reserved</option>
            </select>
        </span>
    </div>
    <div class="associate">
        <span class="{{if isAssociating 'ui-state-disabled'}}">
            <a id={{view.domID}} class="{{if isAssociating 'associating ui-state-disabled' 'ui-icon-carat-d'}}
                    assign-network-btn ui-btn-icon-right ui-mini ui-btn"
                {{action "assignClicked" target=view}}>
                    {{#if server.name}}
                        {{server.name}}
                    {{else}}
                        --
                    {{/if}}
            </a>
        </span>
    </div>
</td>
