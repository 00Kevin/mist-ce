<table class="info-table">
    <tbody>
    {{#if name}}
    <tr>
        <td class="subnet-name">Name</td>
        <td class="subnet-name">{{name}}</td>
    </tr>
    {{/if}}
    {{#if id}}
    <tr>
        <td>Id</td>
        <td>{{id}}</td>
    </tr>
    {{/if}}
    {{#if this.zone}}
    <tr>
        <td>Zone</td>
        <td>{{this.zone.name}}</td>
    </tr>
    {{/if}}
    {{#if this.cidr}}
    <tr>
        <td>Cidr</td>
        <td>{{this.cidr}}</td>
    </tr>
    {{/if}}
    {{#if this.enable_dhcp}}
    <tr>
        <td>DHCP Enabled</td>
        <td>{{this.enable_dhcp}}</td>
    </tr>
    {{/if}}
    {{#if this.gateway_ip}}
    <tr>
        <td>Gateway IP</td>
        <td>{{unbound this.gateway_ip}}</td>
    </tr>
    {{/if}}
    {{#if this.dns_nameservers.length}}
    <tr>
        <td>DNS Nameservers</td>
        <td>
            {{#each this.dns_nameservers as |nameserver|}}
            <div class="ip">{{unbound nameserver}}</div>
            {{/each}}
        </td>
    </tr>
    {{/if}}
    {{#if allocation_pools.length}}
    <tr>
        <td>Allocation pools</td>
        <td>
            {{#each this.allocation_pools as |pool|}}
            <div class="ip">{{unbound pool.start}}</div> -
            <div class="ip">{{unbound pool.end}}</div>
            {{/each}}
        </td>
    </tr>
    {{/if}}

    {{#if ipAddresses}}
    <tr>
        <td class="usable-ips" colspan="2">
            <h4>Usable ip addresses ({{ipAddresses.length}})</h4>
            <table>
                <tbody>
                {{#each ipAddresses as |ip|}}
                    {{view "ipAddressListItem" model=ip}}
                {{/each}}
                </tbody>
            </table>
        </td>
    </tr>
    {{/if}}
    </tbody>
</table>
